ORG 20000
INICIAR:
MOVE #65535, .SP
MOVE .SP, .IX
BR /L_DECREMENTA_FIN
L_DECREMENTA:
MOVE #-3[.IX], .R1
SUB .R1, #1
MOVE .A, #-5[.IX]
MOVE #-5[.IX], #-4[.IX]
WRSTR /texto0
WRINT #-4[.IX]
MOVE .IX, .SP
MOVE [.IX], .IX
MOVE #1[.IX], .R1
BR [.R1]
L_DECREMENTA_FIN:
WRSTR /texto1
WRSTR /SaltoLinea
MOVE #3, /1



MOVE .IX, #-5[.IX]
SUB .IX, #5
MOVE .A, .IX
MOVE .PC, #-4[.IX]
BR /L_DECREMENTA
HALT

texto0: DATA "y(2):"
texto1: DATA "SUBPROGRAMAS PROCEDIMIENTOS"
SaltoLinea: DATA "\n"
