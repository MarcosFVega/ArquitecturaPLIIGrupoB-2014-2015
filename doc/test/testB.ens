ORG 20000
INICIAR:
MOVE #65535, .SP
MOVE .SP, .IX
WRSTR /texto0
WRSTR /SaltoLinea
REG
MOVE #12345, #-0[.IX]
REG
MOVE #1, #-1[.IX]
WRSTR /texto1
REG
WRINT #-2[.IX]
WRSTR /texto2
REG
MOVE #-3[.IX], .R1
OR .R1, #0
BZ /L_0
WRSTR /texto3
BR /L_1
L_0:
WRSTR /texto4
L_1:
HALT

texto0: DATA "TIPOS ESTRUCTURADOS REGISTROS"
texto1: DATA "persona1.dni(12345):"
texto2: DATA "persona1.casado(true):"
texto3: DATA "true"
texto4: DATA "fail"
SaltoLinea: DATA "\n"
